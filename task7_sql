CREATE VIEW public.employee_details AS
 SELECT e.name,
    e.department,
    e.salary,
    e.city
   FROM public.employees e;


ALTER VIEW public.employee_details OWNER TO postgres;

--
-- TOC entry 215 (class 1259 OID 16571)
-- Name: high_salary_employees; Type: VIEW; Schema: public; Owner: postgres
--

CREATE VIEW public.high_salary_employees AS
 SELECT employees.name,
    employees.department,
    employees.salary
   FROM public.employees
  WHERE (employees.salary > 50000);


ALTER VIEW public.high_salary_employees OWNER TO postgres;

--
-- TOC entry 219 (class 1259 OID 16591)
-- Name: it_employees; Type: VIEW; Schema: public; Owner: postgres
--

CREATE VIEW public.it_employees AS
 SELECT employees.id,
    employees.name,
    employees.department,
    employees.salary,
    employees.city
   FROM public.employees
  WHERE (employees.department = 'IT'::text)
  WITH CASCADED CHECK OPTION;


ALTER VIEW public.it_employees OWNER TO postgres;

