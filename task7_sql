--
-- TOC entry 218 (class 1259 OID 16587)
-- Name: above_department_avg; Type: VIEW; Schema: public; Owner: postgres
--

CREATE VIEW public.above_department_avg AS
 SELECT e.name,
    e.department,
    e.salary
   FROM public.employees e
  WHERE ((e.salary)::numeric > ( SELECT avg(d.salary) AS avg
           FROM public.employees d
          WHERE (d.department = e.department)));


ALTER VIEW public.above_department_avg OWNER TO postgres;

--
-- TOC entry 216 (class 1259 OID 16575)
-- Name: department_salary_avg; Type: VIEW; Schema: public; Owner: postgres
--

CREATE VIEW public.department_salary_avg AS
 SELECT employees.department,
    avg(employees.salary) AS avg_salary
   FROM public.employees
  GROUP BY employees.department;


ALTER VIEW public.department_salary_avg OWNER TO postgres;

--
-- TOC entry 217 (class 1259 OID 16579)
